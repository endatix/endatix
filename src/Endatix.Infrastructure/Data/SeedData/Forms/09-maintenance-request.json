{
  "form": {
    "name": "Maintenance Request"
  },
  "definition": {
    "jsonSchema": {
        "title": "Maintenance Request",
        "description": "Use this form to report maintenance issues on the property.",
        "pages": [
            {
            "name": "request-details",
            "title": "Request Details",
            "elements": [
                {
                "type": "text",
                "name": "fullName",
                "title": "Your Full Name",
                "isRequired": true
                },
                {
                "type": "phone",
                "name": "contactPhone",
                "title": "Phone Number",
                "isRequired": true
                },
                {
                "type": "text",
                "name": "email",
                "title": "Email Address",
                "isRequired": true,
                "inputType": "email"
                },
                {
                "type": "text",
                "name": "propertyAddress",
                "title": "Property Address",
                "isRequired": true
                },
                {
                "type": "text",
                "name": "unitNumber",
                "title": "Unit or Apartment Number"
                },
                {
                "type": "dropdown",
                "name": "issueCategory",
                "title": "Type of Issue",
                "isRequired": true,
                "choices": [
                    "Plumbing",
                    "Electrical",
                    "Heating/Cooling",
                    "Appliances",
                    "Pest Control",
                    "Structural/Damage"
                ],
                "showOtherItem": true
                },
                {
                "type": "comment",
                "name": "issueDescription",
                "title": "Describe the issue in detail",
                "isRequired": true
                },
                {
                "type": "radiogroup",
                "name": "urgency",
                "title": "How urgent is this request?",
                "isRequired": true,
                "choices": [
                    "Emergency (safety or property risk)",
                    "Urgent (needs attention soon)",
                    "Routine (not urgent)"
                ]
                },
                {
                "type": "file",
                "name": "photos",
                "title": "Upload photos or documents (optional)",
                "allowMultiple": true,
                "waitForUpload": true,
                "maxSize": 4096000
                }
            ]
            },
            {
            "name": "access-contact-preferences",
            "title": "Access & Contact Preferences",
            "elements": [
                {
                "type": "boolean",
                "name": "accessPermission",
                "title": "Do you allow maintenance staff to enter your unit if you're not present?",
                "isRequired": true,
                "swapOrder": true
                },
                {
                "type": "comment",
                "name": "accessInstructions",
                "title": "Access instructions or special notes (e.g., pets, alarm code)"
                },
                {
                "type": "radiogroup",
                "name": "preferredContactMethod",
                "title": "Preferred contact method if follow-up is needed:",
                "choices": [
                    "Phone",
                    "Email",
                    "Text Message"
                ]
                }
            ]
            },
            {
            "name": "confirmation",
            "title": "Confirmation",
            "elements": [
                {
                "type": "html",
                "name": "thankYou",
                "html": "<p>Thank you for your request. Our team will follow up as soon as possible.</p>"
                }
            ]
            }
        ],
        "showProgressBar": true
        }
  },
