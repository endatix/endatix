{
  "form": {
    "name": "Brand Awareness & Perception Survey"
  },
  "definition": {
    "jsonSchema": {
        "title": "Brand Awareness &amp; Perception Survey",
        "description": "We’d like your honest thoughts about brands in this industry.",
        "pages": [
            {
            "name": "screening",
            "title": "Screening",
            "elements": [
                {
                "type": "radiogroup",
                "name": "usedCategoryRecently",
                "title": "Have you purchased or used any products in this category in the past 6 months?",
                "isRequired": true,
                "choices": [
                    "Yes",
                    "No"
                ]
                }
            ]
            },
            {
            "name": "brand-awareness",
            "visibleIf": "{usedCategoryRecently} = 'Yes'",
            "title": "Brand Awareness",
            "elements": [
                {
                "type": "comment",
                "name": "unaidedAwareness",
                "title": "Which brands come to mind when you think of this product category?"
                },
                {
                "type": "checkbox",
                "name": "aidedAwareness",
                "title": "Which of the following brands have you heard of?",
                "isRequired": true,
                "choices": [
                    "Brand X",
                    "Brand Y",
                    "Brand Z"
                ],
                "showOtherItem": true,
                "showNoneItem": true,
                "noneText": "None of the above",
                "otherText": "Other"
                }
            ]
            },
            {
            "name": "brand-perception",
            "visibleIf": "{usedCategoryRecently} = 'Yes'",
            "title": "Brand Perception",
            "elements": [
                {
                "type": "dropdown",
                "name": "familiarBrand",
                "title": "Which brand are you most familiar with?",
                "isRequired": true,
                "choicesFromQuestion": "aidedAwareness",
                "choices": [
                    "Item 1",
                    "Item 2",
                    "Item 3"
                ]
                },
                {
                "type": "imagepicker",
                "name": "brandFeel",
                "title": "Which of these images best represents how you feel about Brand X?",
                "choices": [
                    {
                    "value": "innovative",
                    "text": "Innovative",
                    "imageLink": "https://example.com/img/innovative.jpg"
                    },
                    {
                    "value": "traditional",
                    "text": "Traditional",
                    "imageLink": "https://example.com/img/traditional.jpg"
                    },
                    {
                    "value": "fun",
                    "text": "Fun",
                    "imageLink": "https://example.com/img/fun.jpg"
                    },
                    {
                    "value": "boring",
                    "text": "Boring",
                    "imageLink": "https://example.com/img/boring.jpg"
                    }
                ],
                "showLabel": true
                },
                {
                "type": "matrix",
                "name": "brandAttributes",
                "title": "How would you rate Brand X on the following attributes?",
                "columns": [
                    {
                    "value": "1",
                    "text": "Very Poor"
                    },
                    {
                    "value": "2",
                    "text": "Poor"
                    },
                    {
                    "value": "3",
                    "text": "Average"
                    },
                    {
                    "value": "4",
                    "text": "Good"
                    },
                    {
                    "value": "5",
                    "text": "Excellent"
                    }
                ],
                "rows": [
                    {
                    "value": "quality",
                    "text": "Product Quality"
                    },
                    {
                    "value": "value",
                    "text": "Value for Money"
                    },
                    {
                    "value": "innovation",
                    "text": "Innovation"
                    },
                    {
                    "value": "trust",
                    "text": "Trustworthiness"
                    },
                    {
                    "value": "customerService",
                    "text": "Customer Service"
                    }
                ]
                },
                {
                "type": "comment",
                "name": "brandWords",
                "title": "What three words would you use to describe Brand X?"
                },
                {
                "type": "radiogroup",
                "name": "recommendation",
                "title": "Would you recommend Brand X to a friend?",
                "choices": [
                    "Yes",
                    "No",
                    "Not sure"
                ]
                }
            ]
            },
            {
            "name": "comparative-perception",
            "visibleIf": "{usedCategoryRecently} = 'Yes'",
            "title": "Comparative Perception",
            "elements": [
                {
                "type": "ranking",
                "name": "brandRanking",
                "title": "Rank up to 5 brands from most to least favorable in your opinion. (You may skip some)",
                "choicesFromQuestion": "aidedAwareness",
                "choices": [
                    "Item 1",
                    "Item 2",
                    "Item 3"
                ],
                "choicesFromQuestionMode": "selected"
                },
                {
                "type": "comment",
                "name": "brandPreferenceReason",
                "title": "What influenced your top choice the most?"
                }
            ]
            },
            {
            "name": "wrap-up",
            "title": "Wrap-Up",
            "elements": [
                {
                "type": "comment",
                "name": "finalThoughts",
                "title": "Any final thoughts on this category or any brand not mentioned?"
                },
                {
                "type": "html",
                "name": "thanks",
                "html": "<p>Thank you for your feedback!</p>"
                }
            ]
            }
        ],
        "showProgressBar": true
        }
  },
