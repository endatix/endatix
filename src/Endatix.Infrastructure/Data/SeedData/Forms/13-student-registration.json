{
  "form": {
    "name": "Student Registration"
  },
  "definition": {
    "jsonSchema": {
        "title": "Student Registration Form",
        "description": "Please complete the form below to register as a student.",
        "pages": [
            {
            "name": "personal-information",
            "title": "Personal Information",
            "elements": [
                {
                "type": "text",
                "name": "firstName",
                "title": "First Name",
                "isRequired": true
                },
                {
                "type": "text",
                "name": "lastName",
                "title": "Last Name",
                "isRequired": true
                },
                {
                "type": "text",
                "name": "dob",
                "title": "Date of Birth",
                "isRequired": true,
                "inputType": "date"
                },
                {
                "type": "radiogroup",
                "name": "gender",
                "title": "Gender",
                "choices": [
                    "Male",
                    "Female"
                ]
                },
                {
                "type": "text",
                "name": "nationality",
                "title": "Nationality"
                },
                {
                "type": "file",
                "name": "idUpload",
                "title": "Upload ID or Passport",
                "isRequired": true,
                "waitForUpload": true,
                "maxSize": 4096000
                }
            ]
            },
            {
            "name": "contact-information",
            "title": "Contact Information",
            "elements": [
                {
                "type": "text",
                "name": "email",
                "title": "Email",
                "isRequired": true,
                "inputType": "email"
                },
                {
                "type": "phone",
                "name": "phone",
                "title": "Phone Number"
                },
                {
                "type": "text",
                "name": "address",
                "title": "Street Address"
                },
                {
                "type": "text",
                "name": "city",
                "title": "City"
                },
                {
                "type": "text",
                "name": "postalCode",
                "title": "Postal Code"
                },
                {
                "type": "text",
                "name": "country",
                "title": "Country"
                }
            ]
            },
            {
            "name": "academic-background",
            "title": "Academic Background",
            "elements": [
                {
                "type": "text",
                "name": "currentSchool",
                "title": "Current or Most Recent School/Institution"
                },
                {
                "type": "dropdown",
                "name": "highestEducation",
                "title": "Highest Level of Education Completed",
                "choices": [
                    "Primary",
                    "Secondary",
                    "High School",
                    "Associate Degree",
                    "Bachelor’s Degree",
                    "Master’s Degree",
                    "Doctorate"
                ]
                },
                {
                "type": "file",
                "name": "transcriptUpload",
                "title": "Upload Transcript (optional)",
                "waitForUpload": true
                }
            ]
            },
            {
            "name": "course-selection",
            "title": "Course Selection",
            "elements": [
                {
                "type": "dropdown",
                "name": "programType",
                "title": "Program or Course Type",
                "isRequired": true,
                "choices": [
                    "Full-time",
                    "Part-time",
                    "Online",
                    "Self-paced"
                ]
                },
                {
                "type": "dropdown",
                "name": "courseInterest",
                "title": "Course of Interest",
                "isRequired": true,
                "choices": [
                    "Item 1",
                    "Item 2",
                    "Item 3"
                ],
                "choicesByUrl": {
                    "url": "https://api.example.com/courses",
                    "valueName": "id",
                    "titleName": "name"
                }
                },
                {
                "type": "text",
                "name": "preferredStartDate",
                "title": "Preferred Start Date",
                "inputType": "date"
                }
            ]
            },
            {
            "name": "emergency-contact",
            "title": "Emergency Contact",
            "elements": [
                {
                "type": "text",
                "name": "emergencyName",
                "title": "Emergency Contact Name",
                "isRequired": true
                },
                {
                "type": "text",
                "name": "emergencyRelation",
                "title": "Relationship to Student"
                },
                {
                "type": "phone",
                "name": "emergencyPhone",
                "title": "Emergency Phone Number",
                "isRequired": true
                }
            ]
            },
            {
            "name": "medical-consent",
            "title": "Medical & Consent",
            "elements": [
                {
                "type": "boolean",
                "name": "hasMedicalConditions",
                "title": "Do you have any medical conditions we should be aware of?",
                "swapOrder": true
                },
                {
                "type": "comment",
                "name": "medicalDetails",
                "visibleIf": "{hasMedicalConditions} = true",
                "title": "Please describe your medical conditions"
                },
                {
                "type": "signaturepad",
                "name": "signature",
                "title": "Signature (for consent)",
                "isRequired": true,
                "waitForUpload": true
                },
                {
                "type": "text",
                "name": "signatureDate",
                "title": "Date of Signature",
                "defaultValueExpression": "today()",
                "isRequired": true,
                "inputType": "date"
                }
            ]
            },
            {
            "name": "confirmation",
            "title": "Confirmation",
            "elements": [
                {
                "type": "html",
                "name": "thankYou",
                "html": "<p>Thank you for registering. We will review your application and contact you shortly.</p>"
                }
            ]
            }
        ],
        "showProgressBar": true
        }
  },
