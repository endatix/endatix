{
  "form": {
    "name": "Course Evaluation Survey"
  },
  "definition": {
    "jsonSchema": {
        "title": "Course Evaluation Survey",
        "description": "We appreciate your feedback on the course. Your responses will help us improve future sessions.",
        "pages": [
            {
            "name": "course-content-delivery",
            "title": "Course Content & Delivery",
            "elements": [
                {
                "type": "text",
                "name": "courseTitle",
                "title": "Course Title",
                "isRequired": true
                },
                {
                "type": "matrix",
                "name": "contentEvaluation",
                "title": "Please rate the following aspects of the course:",
                "columns": [
                    {
                    "value": "1",
                    "text": "Very Poor"
                    },
                    {
                    "value": "2",
                    "text": "Poor"
                    },
                    {
                    "value": "3",
                    "text": "Average"
                    },
                    {
                    "value": "4",
                    "text": "Good"
                    },
                    {
                    "value": "5",
                    "text": "Excellent"
                    }
                ],
                "rows": [
                    {
                    "value": "contentClarity",
                    "text": "Clarity of course content"
                    },
                    {
                    "value": "contentRelevance",
                    "text": "Relevance to your learning goals"
                    },
                    {
                    "value": "materialQuality",
                    "text": "Quality of course materials"
                    },
                    {
                    "value": "pace",
                    "text": "Appropriate pace of delivery"
                    }
                ],
                "eachRowRequired": true
                },
                {
                "type": "rating",
                "name": "contentSatisfaction",
                "title": "Overall satisfaction with course content",
                "isRequired": true
                },
                {
                "type": "comment",
                "name": "lowContentFeedback",
                "visibleIf": "{contentSatisfaction} <= 3",
                "title": "What could be improved in the course content?"
                }
            ]
            },
            {
            "name": "instructor-evaluation",
            "title": "Instructor Evaluation",
            "elements": [
                {
                "type": "text",
                "name": "instructorName",
                "title": "Instructor Name",
                "isRequired": true
                },
                {
                "type": "matrix",
                "name": "instructorEvaluation",
                "title": "Please rate the following aspects of the instructor:",
                "columns": [
                    {
                    "value": "1",
                    "text": "Very Poor"
                    },
                    {
                    "value": "2",
                    "text": "Poor"
                    },
                    {
                    "value": "3",
                    "text": "Average"
                    },
                    {
                    "value": "4",
                    "text": "Good"
                    },
                    {
                    "value": "5",
                    "text": "Excellent"
                    }
                ],
                "rows": [
                    {
                    "value": "knowledge",
                    "text": "Subject knowledge"
                    },
                    {
                    "value": "clarity",
                    "text": "Clarity of explanations"
                    },
                    {
                    "value": "engagement",
                    "text": "Engagement and interaction"
                    },
                    {
                    "value": "responsiveness",
                    "text": "Responsiveness to questions"
                    }
                ],
                "eachRowRequired": true
                },
                {
                "type": "rating",
                "name": "instructorSatisfaction",
                "title": "Overall satisfaction with the instructor",
                "isRequired": true
                },
                {
                "type": "comment",
                "name": "lowInstructorFeedback",
                "visibleIf": "{instructorSatisfaction} <= 3",
                "title": "What could be improved in the instruction?"
                }
            ]
            },
            {
            "name": "overall-experience",
            "title": "Overall Experience",
            "elements": [
                {
                "type": "rating",
                "name": "overallExperience",
                "title": "How would you rate your overall course experience?",
                "isRequired": true,
                "minRateDescription": "Very Dissatisfied",
                "maxRateDescription": "Very Satisfied"
                },
                {
                "type": "radiogroup",
                "name": "wouldRecommend",
                "title": "Would you recommend this course to others?",
                "choices": [
                    "Yes",
                    "No",
                    "Not sure"
                ]
                },
                {
                "type": "comment",
                "name": "finalComments",
                "title": "Any final comments, suggestions, or praise?"
                }
            ]
            },
            {
            "name": "thank-you",
            "title": "Thank You",
            "elements": [
                {
                "type": "html",
                "name": "thanks",
                "html": "<p>Thank you for your feedback!</p><p>Your input is valuable and helps us improve future courses.</p>"
                }
            ]
            }
        ],
        "showProgressBar": true
        }
  },
