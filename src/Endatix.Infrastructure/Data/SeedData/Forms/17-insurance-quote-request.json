{
  "form": {
    "name": "Insurance Quote Request"
  },
  "definition": {
    "jsonSchema": {
        "title": "Insurance Quote Request Form",
        "description": "Please provide the following information to receive an insurance quote.",
        "pages": [
            {
            "name": "basic-information",
            "title": "Basic Information",
            "elements": [
                {
                "type": "dropdown",
                "name": "insuranceType",
                "title": "What type of insurance do you need?",
                "isRequired": true,
                "choices": [
                    "Auto",
                    "Home",
                    "Life"
                ]
                },
                {
                "type": "text",
                "name": "fullName",
                "title": "Full Name",
                "isRequired": true
                },
                {
                "type": "text",
                "name": "email",
                "title": "Email",
                "isRequired": true,
                "inputType": "email"
                },
                {
                "type": "phone",
                "name": "phone"
                }
            ]
            },
            {
            "name": "auto-insurance",
            "visibleIf": "{insuranceType} = 'Auto'",
            "title": "Auto Insurance",
            "elements": [
                {
                "type": "text",
                "name": "vehicleMake",
                "title": "Vehicle Make",
                "isRequired": true
                },
                {
                "type": "text",
                "name": "vehicleModel",
                "title": "Vehicle Model",
                "isRequired": true
                },
                {
                "type": "text",
                "name": "vehicleYear",
                "title": "Year of Manufacture",
                "isRequired": true,
                "inputType": "number"
                },
                {
                "type": "text",
                "name": "vin",
                "title": "Vehicle Identification Number (VIN)"
                },
                {
                "type": "boolean",
                "name": "hasPreviousClaimsAuto",
                "title": "Have you had any claims in the past 5 years?",
                "swapOrder": true
                }
            ]
            },
            {
            "name": "home-insurance",
            "visibleIf": "{insuranceType} = 'Home'",
            "title": "Home Insurance",
            "elements": [
                {
                "type": "text",
                "name": "propertyAddress",
                "title": "Property Address",
                "isRequired": true
                },
                {
                "type": "text",
                "name": "yearBuilt",
                "title": "Year Built",
                "isRequired": true,
                "inputType": "number"
                },
                {
                "type": "dropdown",
                "name": "propertyType",
                "title": "Type of Property",
                "choices": [
                    "Single Family",
                    "Condo",
                    "Townhouse",
                    "Multi-Family"
                ],
                "showOtherItem": true
                },
                {
                "type": "text",
                "name": "estimatedValue",
                "title": "Estimated Property Value (USD)",
                "inputType": "number"
                },
                {
                "type": "boolean",
                "name": "hasSecuritySystem",
                "title": "Does the property have a security system?",
                "swapOrder": true
                }
            ]
            },
            {
            "name": "life-insurance",
            "visibleIf": "{insuranceType} = 'Life'",
            "title": "Life Insurance",
            "elements": [
                {
                "type": "text",
                "name": "dob",
                "title": "Date of Birth",
                "isRequired": true,
                "inputType": "date"
                },
                {
                "type": "radiogroup",
                "name": "gender",
                "title": "Gender",
                "choices": [
                    "Male",
                    "Female"
                ]
                },
                {
                "type": "text",
                "name": "coverageAmount",
                "title": "Desired Coverage Amount (USD)",
                "isRequired": true,
                "inputType": "number"
                },
                {
                "type": "boolean",
                "name": "smoker",
                "title": "Do you smoke?",
                "swapOrder": true
                },
                {
                "type": "comment",
                "name": "healthConditions",
                "title": "Please list any pre-existing health conditions (optional)"
                }
            ]
            },
            {
            "name": "optional-comments",
            "title": "Optional Comments",
            "elements": [
                {
                "type": "comment",
                "name": "additionalNotes",
                "title": "Any additional information or questions?"
                }
            ]
            },
            {
            "name": "confirmation",
            "title": "Confirmation",
            "elements": [
                {
                "type": "html",
                "name": "confirmationText",
                "html": "<h3>Thank you!</h3><p>We have received your request and will reach out shortly with your quote.</p>"
                }
            ]
            }
        ],
        "showProgressBar": true
        }
  },
