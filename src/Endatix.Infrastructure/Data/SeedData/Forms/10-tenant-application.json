{
  "form": {
    "name": "Tenant Application"
  },
  "definition": {
    "jsonSchema": {
        "title": "Tenant Rental Application",
        "description": "Complete this form to apply for a rental unit.",
        "pages": [
            {
            "name": "personal-information",
            "title": "Personal Information",
            "elements": [
                {
                "type": "text",
                "name": "fullName",
                "title": "Full Name",
                "isRequired": true
                },
                {
                "type": "text",
                "name": "dob",
                "title": "Date of Birth",
                "isRequired": true,
                "inputType": "date"
                },
                {
                "type": "text",
                "name": "ssn",
                "title": "Social Security Number (optional)",
                "inputType": "password"
                },
                {
                "type": "phone",
                "name": "phone",
                "title": "Phone Number",
                "isRequired": true
                },
                {
                "type": "text",
                "name": "email",
                "title": "Email Address",
                "isRequired": true,
                "inputType": "email"
                },
                {
                "type": "file",
                "name": "idUpload",
                "title": "Upload a government-issued ID (PDF or image)",
                "isRequired": true,
                "waitForUpload": true,
                "maxSize": 4096000
                },
                {
                "type": "boolean",
                "name": "hasCoApplicant",
                "title": "Will there be a co-applicant?",
                "swapOrder": true
                },
                {
                "type": "text",
                "name": "coApplicantName",
                "visibleIf": "{hasCoApplicant} = true",
                "title": "Co-applicant Full Name"
                }
            ]
            },
            {
            "name": "employment-income",
            "title": "Employment & Income",
            "elements": [
                {
                "type": "text",
                "name": "employerName",
                "title": "Current Employer",
                "isRequired": true
                },
                {
                "type": "text",
                "name": "jobTitle",
                "title": "Job Title"
                },
                {
                "type": "text",
                "name": "monthlyIncome",
                "title": "Monthly Income (in USD)",
                "isRequired": true,
                "inputType": "number"
                },
                {
                "type": "file",
                "name": "incomeProof",
                "title": "Upload proof of income (pay stub or bank statement)",
                "isRequired": true,
                "waitForUpload": true,
                "maxSize": 4096000
                }
            ]
            },
            {
            "name": "rental-history",
            "title": "Rental History",
            "elements": [
                {
                "type": "paneldynamic",
                "name": "pastResidences",
                "title": "Previous Residences",
                "templateElements": [
                    {
                    "type": "text",
                    "name": "address",
                    "title": "Address",
                    "isRequired": true
                    },
                    {
                    "type": "text",
                    "name": "landlordName",
                    "title": "Landlord/Manager Name"
                    },
                    {
                    "type": "phone",
                    "name": "landlordPhone",
                    "title": "Landlord Phone"
                    },
                    {
                    "type": "text",
                    "name": "duration",
                    "title": "How long did you live there?"
                    }
                ],
                "addPanelText": "Add Residence"
                }
            ]
            },
            {
            "name": "references-pets",
            "title": "References & Pets",
            "elements": [
                {
                "type": "paneldynamic",
                "name": "references",
                "title": "Personal or Professional References",
                "templateElements": [
                    {
                    "type": "text",
                    "name": "refName",
                    "title": "Name",
                    "isRequired": true
                    },
                    {
                    "type": "phone",
                    "name": "refPhone",
                    "title": "Phone"
                    },
                    {
                    "type": "text",
                    "name": "refRelationship",
                    "title": "Relationship"
                    }
                ],
                "templateTitle": "Reference #{panelIndex}",
                "addPanelText": "Add Reference"
                },
                {
                "type": "boolean",
                "name": "hasPets",
                "title": "Do you have pets?",
                "swapOrder": true
                },
                {
                "type": "comment",
                "name": "petDetails",
                "visibleIf": "{hasPets} = true",
                "title": "Please describe your pets (type, breed, size)"
                }
            ]
            },
            {
            "name": "consent-signature",
            "title": "Consent & Signature",
            "elements": [
                {
                "type": "html",
                "name": "consentText",
                "html": "<p>I hereby declare that the information provided is true and authorize background and credit checks as part of this application.</p>"
                },
                {
                "type": "signaturepad",
                "name": "applicantSignature",
                "title": "Signature",
                "isRequired": true,
                "waitForUpload": true
                },
                {
                "type": "text",
                "name": "signDate",
                "title": "Date",
                "defaultValueExpression": "today()",
                "isRequired": true,
                "inputType": "date"
                }
            ]
            }
        ],
        "showProgressBar": true
        }
  },
