{
  "form": {
    "name": "Appointment Scheduling"
  },
  "definition": {
    "jsonSchema": {
        "title": "Request an Appointment",
        "description": "Fill out the form to schedule your next medical appointment.",
        "pages": [
            {
            "name": "patient-information",
            "title": "Patient Information",
            "elements": [
                {
                "type": "text",
                "name": "fullName",
                "title": "Full Name",
                "isRequired": true
                },
                {
                "type": "text",
                "name": "dateOfBirth",
                "title": "Date of Birth",
                "isRequired": true,
                "inputType": "date"
                },
                {
                "type": "radiogroup",
                "name": "isReturning",
                "title": "Are you a returning patient?",
                "isRequired": true,
                "choices": [
                    "Yes",
                    "No"
                ]
                },
                {
                "type": "phone",
                "name": "phone",
                "isRequired": true
                },
                {
                "type": "text",
                "name": "email",
                "title": "Email Address",
                "isRequired": true,
                "inputType": "email"
                },
                {
                "type": "text",
                "name": "preferredContactTime",
                "title": "When is the best time to reach you (if needed)?"
                }
            ]
            },
            {
            "name": "appointment-details",
            "title": "Appointment Details",
            "elements": [
                {
                "type": "dropdown",
                "name": "appointmentType",
                "title": "Type of Appointment",
                "isRequired": true,
                "choices": [
                    "General Check-Up",
                    "Consultation",
                    "Lab Test",
                    "Immunization",
                    "Follow-Up",
                    "Other"
                ]
                },
                {
                "type": "comment",
                "name": "reason",
                "title": "Briefly describe the reason for your visit"
                },
                {
                "type": "dropdown",
                "name": "preferredProvider",
                "title": "Preferred Provider",
                "choices": [
                  "Dr. Sarah Mitchell (Family Medicine)",
                  "Dr. James Chen (Internal Medicine)",
                  "Dr. Emily Rodriguez (Pediatrics)",
                  "Dr. Michael Thompson (Cardiology)",
                  "Dr. Lisa Patel (Dermatology)",
                  "Dr. Robert Williams (Orthopedics)",
                  "No Preference"
                ]
                },
                {
                "type": "text",
                "name": "preferredDate",
                "title": "Preferred Date",
                "isRequired": true,
                "inputType": "date"
                },
                {
                "type": "text",
                "name": "preferredTime",
                "title": "Preferred Time",
                "inputType": "time"
                },
                {
                "type": "radiogroup",
                "name": "isFlexible",
                "title": "Are you flexible with the time?",
                "choices": [
                    "Yes",
                    "No"
                ]
                }
            ]
            },
            {
            "name": "insurance-files",
            "title": "Insurance & Files",
            "elements": [
                {
                "type": "radiogroup",
                "name": "hasInsurance",
                "title": "Do you have health insurance?",
                "choices": [
                    "Yes",
                    "No"
                ]
                },
                {
                "type": "panel",
                "name": "insuranceDetails",
                "visibleIf": "{hasInsurance} = 'Yes'",
                "elements": [
                    {
                    "type": "text",
                    "name": "insuranceProvider",
                    "title": "Insurance Provider"
                    },
                    {
                    "type": "text",
                    "name": "insuranceId",
                    "title": "Insurance ID / Member Number"
                    },
                    {
                    "type": "file",
                    "name": "insuranceCard",
                    "title": "Upload your insurance card (optional)",
                    "waitForUpload": true,
                    "maxSize": 2048000
                    }
                ]
                },
                {
                "type": "file",
                "name": "referralDocuments",
                "title": "Upload any relevant medical documents (optional)",
                "allowMultiple": true,
                "waitForUpload": true
                }
            ]
            },
            {
            "name": "confirmation",
            "title": "Confirmation",
            "elements": [
                {
                "type": "html",
                "name": "confirmationNote",
                "html": "<p>Once submitted, our staff will contact you to confirm your appointment details. Thank you!</p>"
                }
            ]
            }
        ],
        "showProgressBar": true
        }
  },
