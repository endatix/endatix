{
  "form": {
    "name": "Claims Submission"
  },
  "definition": {
    "jsonSchema": {
        "title": "Insurance Claims Submission",
        "description": "Please fill out the form below to submit your claim.",
        "pages": [
            {
            "name": "claim-type",
            "title": "Claim Type",
            "elements": [
                {
                "type": "dropdown",
                "name": "claimType",
                "title": "What type of claim are you submitting?",
                "isRequired": true,
                "choices": [
                    "Auto",
                    "Home",
                    "Life",
                    "Health"
                ]
                },
                {
                "type": "text",
                "name": "policyNumber",
                "title": "Policy Number",
                "isRequired": true
                },
                {
                "type": "text",
                "name": "fullName",
                "title": "Full Name",
                "isRequired": true
                },
                {
                "type": "text",
                "name": "email",
                "title": "Email Address",
                "isRequired": true,
                "inputType": "email"
                },
                {
                "type": "phone",
                "name": "phone"
                }
            ]
            },
            {
            "name": "incident-details",
            "title": "Incident Details",
            "elements": [
                {
                "type": "text",
                "name": "incidentDate",
                "title": "Date of Incident",
                "isRequired": true,
                "inputType": "date"
                },
                {
                "type": "comment",
                "name": "incidentDescription",
                "title": "Describe what happened",
                "isRequired": true
                },
                {
                "type": "file",
                "name": "incidentPhotos",
                "title": "Upload photos or documents related to the incident",
                "allowMultiple": true,
                "waitForUpload": true,
                "maxSize": 10485760
                }
            ]
            },
            {
            "name": "auto-claim-details",
            "visibleIf": "{claimType} = 'Auto'",
            "title": "Auto Claim Details",
            "elements": [
                {
                "type": "text",
                "name": "vehicleMake",
                "title": "Vehicle Make"
                },
                {
                "type": "text",
                "name": "vehicleModel",
                "title": "Vehicle Model"
                },
                {
                "type": "text",
                "name": "licensePlate",
                "title": "License Plate Number"
                },
                {
                "type": "boolean",
                "name": "wasPoliceReportFiled",
                "title": "Was a police report filed?",
                "swapOrder": true
                }
            ]
            },
            {
            "name": "home-claim-details",
            "visibleIf": "{claimType} = 'Home'",
            "title": "Home Claim Details",
            "elements": [
                {
                "type": "text",
                "name": "propertyAddress",
                "title": "Property Address"
                },
                {
                "type": "boolean",
                "name": "isHomeHabitable",
                "title": "Is the home currently habitable?",
                "swapOrder": true
                },
                {
                "type": "text",
                "name": "estimatedDamageCost",
                "title": "Estimated Damage Cost (USD)",
                "inputType": "number"
                }
            ]
            },
            {
            "name": "life-claim-details",
            "visibleIf": "{claimType} = 'Life'",
            "title": "Life Claim Details",
            "elements": [
                {
                "type": "text",
                "name": "deceasedName",
                "title": "Name of the Deceased"
                },
                {
                "type": "text",
                "name": "deceasedDOB",
                "title": "Date of Birth",
                "inputType": "date"
                },
                {
                "type": "text",
                "name": "dateOfDeath",
                "title": "Date of Death",
                "inputType": "date"
                },
                {
                "type": "file",
                "name": "deathCertificate",
                "title": "Upload Death Certificate",
                "waitForUpload": true,
                "maxSize": 10485760
                }
            ]
            },
            {
            "name": "health-claim-details",
            "visibleIf": "{claimType} = 'Health'",
            "title": "Health Claim Details",
            "elements": [
                {
                "type": "text",
                "name": "patientName",
                "title": "Patient's Name"
                },
                {
                "type": "text",
                "name": "serviceDate",
                "title": "Date of Service",
                "inputType": "date"
                },
                {
                "type": "text",
                "name": "providerName",
                "title": "Healthcare Provider Name"
                },
                {
                "type": "file",
                "name": "medicalBills",
                "title": "Upload Bills or Receipts",
                "allowMultiple": true,
                "waitForUpload": true,
                "maxSize": 10485760
                }
            ]
            },
            {
            "name": "additional-notes",
            "title": "Additional Notes",
            "elements": [
                {
                "type": "comment",
                "name": "additionalInfo",
                "title": "Any additional information you would like to provide?"
                }
            ]
            },
            {
            "name": "submission-confirmation",
            "title": "Submission Confirmation",
            "elements": [
                {
                "type": "html",
                "name": "confirmation",
                "html": "<h3>Thank you for your submission!</h3><p>Our team will review your claim and get back to you as soon as possible.</p>"
                }
            ]
            }
        ],
        "showProgressBar": true
        }
  },
