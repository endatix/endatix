{
  "trailingSlash": "auto",
  "routes": [
    {
      "route": "/docs/getting-started/installation",
      "redirect": "/docs/getting-started/setup-nuget-package",
      "statusCode": 301
    },
    {
      "route": "/docs/building-your-solution/hosting",
      "redirect": "/docs/building-your-solution",
      "statusCode": 301
    },
    {
      "route": "/docs/settings/",
      "redirect": "/docs/configuration/settings/",
      "statusCode": 301
    },
    {
      "route": "/docs/configuration/settings/jwt-settings",
      "redirect": "docs/configuration/settings/auth-settings",
      "statusCode": 301
    },
    {
      "route": "/docs/settings/cors-settings",
      "redirect": "/docs/configuration/settings/cors-settings",
      "statusCode": 301
    },
    {
      "route": "/docs/settings/data-settings",
      "redirect": "/docs/configuration/settings/data-settings",
      "statusCode": 301
    },
    {
      "route": "/docs/settings/health-settings",
      "redirect": "/docs/configuration/settings/health-settings",
      "statusCode": 301
    },
    {
      "route": "/docs/settings/jwt-settings",
      "redirect": "/docs/configuration/settings/jwt-settings",
      "statusCode": 301
    },
    {
      "route": "/docs/settings/persistence-settings",
      "redirect": "/docs/configuration/settings/persistence-settings",
      "statusCode": 301
    },
    {
      "route": "/docs/getting-started/setup-docker",
      "redirect": "/docs/guides/docker-setup",
      "statusCode": 301
    }
  ],
  "globalHeaders": {
    "content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline'; object-src 'none'; worker-src blob:; img-src https: data:"
  },
  "mimeTypes": {
    ".json": "text/json"
  }
}
